    "def iniciando_navegador(url):\n",
    "    # Configurando o serviço do ChromeDriver e iniciando o navegador\n",
    "    servico = Service(ChromeDriverManager().install())\n",
    "    navegador = webdriver.Chrome(service=servico)\n",
    "    \n",
    "    # Acessando a URL fornecida e maximizando a janela do navegador\n",
    "    navegador.get(url)\n",
    "    navegador.maximize_window()\n",
    "    \n",
    "    # Retornando o objeto do navegador\n",
    "    return navegador"
   ]
