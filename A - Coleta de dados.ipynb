    "# Criando um objeto BeautifulSoup para analisar o código-fonte da página atual no navegador\n",
    "soup = BeautifulSoup(navegador.page_source, \"html.parser\")\n",
    "\n",
    "# Encontrando a tabela com a classe 'lbl' no HTML analisado\n",
    "tabela = soup.find('table', {'class': 'lbl'})\n",
    "\n",
    "# Convertendo a tabela em string HTML\n",
    "tabela_string = str(tabela)\n",
    "\n",
    "#Criando o DataFrame\n",
    "df_municipios_habitantes = pd.read_html(tabela_string)[0]\n",
    "\n",
    "# Excluir a primeira linha (cabeçalho atual) e a última linha do DataFrame \"df_municipios_habitantes\"\n",
    "df_municipios_habitantes = df_municipios_habitantes.iloc[1:]\n",
    "df_municipios_habitantes = df_municipios_habitantes.iloc[:-1]\n",
    "\n",
    "\n",
    "\n",
    "# Definindo a segunda linha do DataFrame \"df_municipios_habitantes\"\n",
    "df_municipios_habitantes.columns = df_municipios_habitantes.iloc[0]\n",
    "\n",
    "\n",
    "\n",
    "# Removendo o nome do índice do cabeçalho\n",
    "#df_municipios_habitantes.columns.name = None\n",
    "\n",
    "\n",
    "\n",
    "# Definindo a primeira coluna (com os nomes dos municípios) como o índice\n",
    "df_municipios_habitantes = df_municipios_habitantes.iloc[1:]\n",
    "\n",
    "\n",
    "\n",
    "# Removendo o nome do índice (MUNICIPIO)\n",
    "df_municipios_habitantes.index.name = None\n",
    "\n",
    "# Fechando o navegador após a conclusão da coleta de dados\n",
    "navegador.quit()\n",
    "\n",
    "df_municipios_habitantes.head()"
   ]
